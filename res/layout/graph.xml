<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:katz="http://schemas.android.com/apk/res/com.byronkatz"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:weightSum="1">

	<LinearLayout android:orientation="vertical"
		android:layout_width="fill_parent" android:layout_height="75dp">
		<LinearLayout android:layout_height="40dip"
			android:layout_width="fill_parent">
			<EditText android:textSize="10dp" android:fadingEdge="none"
				android:layout_width="60dip" android:layout_gravity="left"
				android:layout_height="fill_parent" android:inputType="numberDecimal"
				android:layout_weight="0.07" android:id="@+id/minValueEditText"
				android:editable="true">
				<requestFocus></requestFocus>
			</EditText>
			<EditText android:textSize="10dp" android:fadingEdge="none"
				android:layout_width="wrap_content" android:layout_height="fill_parent"
				android:inputType="numberDecimal" android:layout_weight="1"
				android:id="@+id/currentValueEditText" android:editable="false"></EditText>
			<EditText android:textSize="10dp" android:fadingEdge="none"
				android:layout_width="60dip" android:layout_gravity="right"
				android:layout_height="fill_parent" android:inputType="numberDecimal"
				android:layout_weight="0.07" android:id="@+id/maxValueEditText"
				android:editable="true"></EditText>
		</LinearLayout>
		<Spinner android:layout_height="40dp" android:id="@+id/valueSpinner"
			android:layout_width="fill_parent"></Spinner>
	</LinearLayout>

	<ScrollView android:id="@+id/graphDataScrollView"
		android:layout_width="fill_parent" android:layout_height="0dp"
		android:layout_weight="1">
		<LinearLayout android:layout_width="fill_parent"
			android:orientation="vertical" android:layout_height="wrap_content">

			<LinearLayout android:id="@+id/graphLinearLayout"
				android:layout_width="fill_parent" android:layout_height="0dp"
				android:orientation="vertical" android:layout_weight="0.60">
				<com.byronkatz.AnalysisGraph
					android:layout_width="fill_parent" android:id="@+id/npvFrameLayout"
					android:layout_height="100dp" katz:graphType="NPV"></com.byronkatz.AnalysisGraph>
				<com.byronkatz.AnalysisGraph
					android:layout_width="fill_parent" android:id="@+id/atcfFrameLayout"
					android:layout_height="100dp" katz:graphType="ATCF"></com.byronkatz.AnalysisGraph>
				<com.byronkatz.AnalysisGraph
					android:layout_width="fill_parent" android:id="@+id/aterFrameLayout"
					android:layout_height="100dp" katz:graphType="ATER"></com.byronkatz.AnalysisGraph>
			</LinearLayout>


			<TableLayout android:layout_height="0dp"
				android:layout_width="fill_parent" android:stretchColumns="1"
				android:gravity="clip_vertical" android:layout_weight="0.40">
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Year"></TextView>
					<TextView android:id="@+id/yearGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="0"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="After Tax Equity Reversion"></TextView>
					<TextView android:id="@+id/aterGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="After Tax Cash Flow"></TextView>
					<TextView android:id="@+id/atcfGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Net Present Value"></TextView>
					<TextView android:id="@+id/npvGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Total Purchase Value"></TextView>
					<TextView android:id="@+id/tpvGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Yearly Interest Rate"></TextView>
					<TextView android:id="@+id/yirGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Building Value"></TextView>
					<TextView android:id="@+id/bvGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Number of compounding periods"></TextView>
					<TextView android:id="@+id/nocpGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Inflation Rate"></TextView>
					<TextView android:id="@+id/irGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Street Address"></TextView>
					<TextView android:id="@+id/saGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="City"></TextView>
					<TextView android:id="@+id/cityGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="State"></TextView>
					<TextView android:id="@+id/siGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Estimated rent payments"></TextView>
					<TextView android:id="@+id/erpGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Real Estate Appreciation Rate"></TextView>
					<TextView android:id="@+id/rearGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Yearly Home Insurance"></TextView>
					<TextView android:id="@+id/yhiGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Property Tax Rate"></TextView>
					<TextView android:id="@+id/ptrGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Local Municipal Fees"></TextView>
					<TextView android:id="@+id/lmfGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Vacancy and Credit Loss Rate"></TextView>
					<TextView android:id="@+id/vaclrGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Initial Yearly General Expenses"></TextView>
					<TextView android:id="@+id/iygeGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Marginal Tax Rate"></TextView>
					<TextView android:id="@+id/mtrGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Selling Broker Rate"></TextView>
					<TextView android:id="@+id/sbrGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="General Sale Expenses"></TextView>
					<TextView android:id="@+id/gseGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Required Rate of Return"></TextView>
					<TextView android:id="@+id/rrrGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Fix-up Costs"></TextView>
					<TextView android:id="@+id/fucGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#FF909090"></View>
				<TableRow android:background="#FF909090">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Closing Costs"></TextView>
					<TextView android:id="@+id/ccGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>
				<View android:layout_height="1dip" android:background="#9d7c62"></View>
				<TableRow android:background="#afe6e7">
					<TextView android:textColor="#000000" android:padding="3dip"
						android:text="Down Payment"></TextView>
					<TextView android:id="@+id/dpGridTextView"
						android:gravity="right" android:textColor="#000000"
						android:padding="3dip" android:text="$0.00"></TextView>
				</TableRow>


			</TableLayout>
		</LinearLayout>
	</ScrollView>

	<LinearLayout android:layout_width="fill_parent"
		android:orientation="vertical" android:id="@+id/sliderLinearLayout"
		android:layout_gravity="bottom" android:layout_height="wrap_content">
		<SeekBar android:layout_height="wrap_content" android:padding="5dip"
			android:layout_width="fill_parent" android:id="@+id/valueSlider"
			android:layout_marginLeft="6dip" android:layout_marginRight="6dip"></SeekBar>
		<SeekBar android:layout_height="wrap_content" android:padding="5dip"
			android:layout_width="fill_parent" android:id="@+id/timeSlider"
			android:layout_marginLeft="6dip" android:layout_marginRight="6dip"></SeekBar>
	</LinearLayout>

</LinearLayout>
