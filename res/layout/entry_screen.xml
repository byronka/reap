<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent" android:gravity="fill">
    <LinearLayout
        android:id="@+id/overallEntryButtonLinearLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:orientation="vertical" android:layout_gravity="center_horizontal" android:layout_margin="20dip">
        <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/setDataValuesButton" android:text="@string/setDataValuesButtonText" android:onClick="setDataValues"></Button>
        <Button android:layout_width="wrap_content" android:id="@+id/viewGraphButton" android:layout_height="wrap_content" android:text="@string/viewGraphButtonText" android:layout_gravity="center_horizontal" android:lines="3" android:onClick="viewGraph"></Button>
    </LinearLayout>
   
</LinearLayout>
